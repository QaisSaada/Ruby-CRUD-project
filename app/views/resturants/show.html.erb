
<div class="jumbotron">
  <h1 class="display-4"> <%= @resturant.name %> </h1>
  <h2 class="display-6"> <%= @resturant.food_type %> </h2> 
  <p class="lead"><%= @resturant.phone %></p>
  <p class="lead"><%= @resturant.location %></p>
  <p class="lead"><%= @resturant.somo %></p>
  <p class="lead">
  <%= link_to "Edit this resturant", edit_resturant_path(@resturant), class:"btn btn-primary" %> 
  </p>
  <p class="lead">
  <%= link_to "Back to resturants", resturants_path, class:"btn btn-primary" %>
  </p>
  <p class="lead">
  <%= button_to "Delete this resturant", @resturant, method: :delete, class:"btn btn-danger" %>
  </p>
</div>


